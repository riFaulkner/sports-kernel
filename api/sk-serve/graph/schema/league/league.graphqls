type League {
    id: ID!
    leagueName: String!
    logoUrl: String!
    startDate: Time!
    teams: [Team!]!
    divisions: [Division!]!
}

type LeagueRoles {
    id: ID!
    userId: ID!
    role: String!
}

input NewUserRole {
    userId: ID!
    role: String!
}

type Division {
    divisionName: String!
    leadingWins: Int
}

type LeagueMutations {
    createUserRole(newUserRole: NewUserRole): LeagueRoles
}