# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  ownerName: String!
  email: String!
  avatar: String!
}

type Query {
  users: [User]
}

input NewUser {
  ownerName: String!
  email: String!
  avatar: String!
}

type Mutation {
  createUser(input: NewUser!): User!
}